    {
      "@id": "dqv:hasQualityAnnotation",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:comment": {
        "@value": "This property describes any quality aspects regarding the delivered content, in particular methods, metrics/indicators and results of a quality assessment in the responsibility of the “dct:rightsHolder”. This information shall assist data consumers in determining the value of data, before actually accessing and processing the data. Thus, The information SHOULD be publicly available. Furthermore, it can be helpful for validation processes by 3rd parties, e.g., a National Body in context of EU Delegated Regulations. Quality aspects SHOULD be noted via a free text and/or via URLs linking to further quality information. If there is absolutely no quality information, at least a note “quality information is unknown” SHOULD be give. If existing quality frameworks have been applied in the quality assessment, e.g., the Quality Packages from the EU EIP and NAPCORE projects [EU-EIP-QP], these frameworks should be referenced. ",
        "@language": "en"
      },
      "rdfs:label": {
        "@value": "quality description",
        "@language": "en"
      },
      "rdfs:isDefinedBy": {
        "@id": "mobilitydcatap:"
      },
      "vs:term_status": "stable",
      "skos:scopeNote": {
        "@value": "In mobilityDCAT-AP, this is an optional property to be used for Datasets.",
        "@language": "en"
      },
      "adms:status": {
        "@id": "http://publications.europa.eu/resource/dataset/concept-status/CURRENT"
      },
      "dcam:domainIncludes": {
        "@id": "dcat:Dataset"
      },
      "dcam:rangeIncludes": {
        "@id": "dqv:QualityAnnotation"
      }
    },
    {
      "@id": "mobilitydcatap:serviceCategory",
      "@type": [
        "rdf:Property",
        "owl:ObjectProperty"
      ],
      "rdfs:comment": {
        "@value": "This property refers to predefined categopries of information services for multimodal mobility, in accordance to possible services listed in EC Delegated Regulation 2017/1926 [EC-MMTIS-DR]. In particular, this property SHOULD describe, if a dataset is intended to support specific services listed in this EC Regulation. Examples for such services are “location search”, which is based on a data set describing address identifiers, or “trip plan computation”, which is based on a data set describing the road network. It might be used to declare for which sercive the dataset is intended for.",
        "@language": "en"
